{
  "name": "code-base-react",
  "description": "This template should help get you started developing with React in Vite",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "main": "src/main.tsx",
  "author": {
    "name": "HXRRY",
    "email": "harrydng1209@gmail.com",
    "url": "https://github.com/harrydng1209"
  },
  "engines": {
    "node": ">=20.x",
    "npm": ">=10.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier . --write",
    "lint": "eslint . --fix",
    "type-check": "tsc --build --force",
    "check-all": "run-p format lint type-check",
    "preinstall": "npx -y only-allow pnpm",
    "prepare": "husky",
    "precommit": "lint-staged && validate-branch-name"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm run format",
      "pnpm run lint",
      "git add ."
    ]
  },
  "validate-branch-name": {
    "pattern": "^(feature|bugfix|hotfix|release)/.+|(master)$"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@reactuses/core": "^6.0.1",
    "antd": "^5.23.0",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "i18next": "^24.2.0",
    "lodash-es": "^4.17.21",
    "qs": "^6.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-hook-form-antd": "^1.1.3",
    "react-i18next": "^15.4.0",
    "react-router": "^7.1.1",
    "string-template": "^1.0.0",
    "uuid": "^11.0.3",
    "yup": "^1.6.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.18.0",
    "@svgr/rollup": "^8.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/qs": "^6.9.17",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/string-template": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-perfectionist": "^4.6.0",
    "eslint-plugin-react": "^7.37.3",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "sass": "1.77.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "unplugin-auto-import": "^0.19.0",
    "validate-branch-name": "^1.3.1",
    "vite": "^6.0.5"
  }
}
